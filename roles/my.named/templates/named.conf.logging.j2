# {{ ansible_managed }}

logging {
{% for channel in my_named_slave_logging_channels %}
  channel {{ channel.name }} {
    file "{{ channel.file }}" versions 3 size 5m;
    severity dynamic;
    print-time yes;
  };
{% endfor %}

{% for category in my_named_slave_logging_categories %}
  category {{ category.name }} { {{ category.channels | join("; ") }}; };
{% endfor %}
};
